{
  "type": "module",
  "name": "@hazae41/berith",
  "version": "1.1.26",
  "author": "hazae41",
  "license": "Unlicense",
  "description": "WebAssembly port of Dalek's Ed25519 and X25519, Rust implementations of Ed25519 signatures and X25519 key exchange",
  "homepage": "https://github.com/hazae41/berith",
  "types": "./dist/types/node/index.d.ts",
  "main": "./dist/cjs/node/index.cjs",
  "sideEffects": false,
  "scripts": {
    "build:wasm": "cd wasm && rimraf pkg && wasm-pack build --target web --release",
    "build:dist": "rimraf dist && deno run -A build.ts && rollup -c",
    "build": "npm run build:wasm && npm run build:dist"
  },
  "peerDependencies": {
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/node": "^18.8.4",
    "rimraf": "^3.0.2",
    "rollup-plugin-dts": "^4.2.3",
    "ts-node": "^10.9.1",
    "tslib": "*",
    "ttypescript": "^1.5.13",
    "typescript": "^4.8.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "exports": {
    ".": {
      "types": "./dist/types/node/index.d.ts",
      "import": "./dist/cjs/node/index.cjs",
      "require": "./dist/cjs/node/index.cjs"
    }
  }
}
